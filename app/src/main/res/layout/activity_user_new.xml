<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".user_new"
    android:id="@+id/fragment_container_user">

    <com.ismaeldivita.chipnavigation.ChipNavigationBar
        android:id="@+id/bottom_nav_menu_user"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cnb_menuResource="@menu/bottom_nav_menu_user"
        android:layout_alignParentBottom="true"
        android:background="@drawable/round_corners_navbar"
        app:cnb_unselectedColor="#000000"
        app:cnb_radius="8dp"
        app:cnb_orientationMode="horizontal"
        android:elevation="8dp"/>
    <FrameLayout
        android:id="@+id/container_user"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@id/bottom_nav_menu_user"/>

</RelativeLayout>